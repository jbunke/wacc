#!/bin/bash

failed=0
total=0

verifyValidFile(){
    ./compile "$1" >/dev/null
    code=$?
    ((total=total+1))
    if [ $code -ne 0 ]; then
        (( failed = failed + 1 ))
        echo "Error: Compiler returned $code instead of 0 for $1"
    fi
}

verifySyntaxErrFile(){
    ./compile "$1" >/dev/null
    code=$?
    ((total=total+1))
    if [ $code -ne 100 ]; then
        (( failed = failed + 1 ))
        echo "Error: Compiler returned $code instead of 100 for $1"
    fi
}


verifySemanticErrFile(){
    ./compile "$1" >/dev/null
    code=$?
    ((total=total+1))
    if [ $code -ne 200 ]; then
        (( failed = failed + 1 ))
        echo "Error: Compiler returned $code instead of 200 for $1"
    fi
}

rm -fr *.s

